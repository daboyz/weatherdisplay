                         lLabelW + labelValueSpacing + lowW;
    
    display.getTextBounds(highStr, 0, 0, &x1, &y1, &w, &h);
    startX = rightSectionStart + (rightSectionWidth - row3TotalWidth) / 2 - x1;
    
