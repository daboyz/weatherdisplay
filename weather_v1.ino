    } else {
      display.setTextColor(GxEPD_BLACK);
    }
    display.setCursor(startX + windIconW + windIconSpace, row2Y);
    display.print(windStr);
    
    // Precipitation value
    display.setTextColor(GxEPD_BLACK);
    int precipX = startX + windIconW + windIconSpace + windValueW + windPrecipSpacing;
