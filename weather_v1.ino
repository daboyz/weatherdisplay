    display.setTextColor(GxEPD_BLACK);
    display.setCursor(startX, row3Y);
    display.print("H");
    
    if (highTemp > 0) {
      display.setTextColor(GxEPD_RED);
    }
    display.setCursor(startX + hLabelW + labelValueSpacing, row3Y);
    display.print(highStr);
    
    // Low temp
    display.setTextColor(GxEPD_BLACK);
    int lowLabelX = startX + hLabelW + labelValueSpacing + highW + highLowSpacing;
    display.setCursor(lowLabelX, row3Y);
    display.print("L");
    
    if (lowTemp > 0) {
      display.setTextColor(GxEPD_RED);
    }
    display.setCursor(lowLabelX + lLabelW + labelValueSpacing, row3Y);
    display.print(lowStr);
    
    // === LEFT SECTION: Dinosaur (bottom) ===
    int dinoY = 88;
    int dinoX;
    const unsigned char* dinoIcon = nullptr;
    
    switch(dinoPosition) {
      case 0: dinoX = dinoLeftX; dinoIcon = icon_dino_left; break;
      case 1: dinoX = dinoCenterX; dinoIcon = icon_dino_center; break;
      case 2: dinoX = dinoRightX; dinoIcon = icon_dino_right; break;
      default: dinoX = dinoCenterX; dinoIcon = icon_dino_center;
    }
    
    display.drawBitmap(dinoX, dinoY, dinoIcon, 48, 32, GxEPD_WHITE, GxEPD_BLACK);
    
  } while (display.nextPage());
  
  Serial.println("Display complete");
  display.hibernate();
}

void drawWeatherIcon(const char* iconCode, int x, int y) {
  const unsigned char* iconBitmap = nullptr;
  
  // Thunderstorm (11d, 11n)
  if (strncmp(iconCode, "11", 2) == 0) {
    iconBitmap = icon_thunderstorm;
  }
  // Rain/Drizzle (09d, 09n, 10d, 10n)
  else if (strncmp(iconCode, "09", 2) == 0 || strncmp(iconCode, "10", 2) == 0) {
    iconBitmap = icon_rain;
  }
  // Snow (13d, 13n)
  else if (strncmp(iconCode, "13", 2) == 0) {
    iconBitmap = icon_snow;
  }
  // Mist/Fog (50d, 50n)
  else if (strncmp(iconCode, "50", 2) == 0) {
    iconBitmap = icon_fog;
  }
  // Clear sky - day (01d)
  else if (strcmp(iconCode, "01d") == 0) {
    iconBitmap = icon_clear;
  }
  // Clear sky - night (01n)
  else if (strcmp(iconCode, "01n") == 0) {
    iconBitmap = icon_clear_night;
  }
  // Few clouds - day (02d)
  else if (strcmp(iconCode, "02d") == 0) {
    iconBitmap = icon_partly_cloudy;
  }
  // Few clouds - night (02n)
  else if (strcmp(iconCode, "02n") == 0) {
    iconBitmap = icon_partly_cloudy_night;
  }
  // Scattered clouds - day (03d) - 25-50% coverage
  else if (strcmp(iconCode, "03d") == 0) {
    iconBitmap = icon_mostly_cloudy;
  }
  // Scattered clouds - night (03n) - 25-50% coverage
  else if (strcmp(iconCode, "03n") == 0) {
    iconBitmap = icon_mostly_cloudy_night;
  }
  // Broken/Overcast clouds (04d, 04n) - 51-100% coverage
  else if (strncmp(iconCode, "04", 2) == 0) {
    iconBitmap = icon_cloudy;
  }
  
  if (iconBitmap != nullptr) {
    display.drawBitmap(x, y, iconBitmap, 60, 60, GxEPD_WHITE, GxEPD_BLACK);
  }
}

void displayError() {
  Serial.println("Displaying error...");
  
  display.setFullWindow();
  display.firstPage();
  
  do {
    display.fillScreen(GxEPD_WHITE);
    display.setTextColor(GxEPD_RED);
    display.setFont(&FreeSansBold12pt7b);
    display.setCursor(10, 40);
    display.println("Error!");
    display.setFont(&FreeSansBold9pt7b);
    display.setCursor(10, 70);
    display.println("Check WiFi");
  } while (display.nextPage());
  
  display.hibernate();
}